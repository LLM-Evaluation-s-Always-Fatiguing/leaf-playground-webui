{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "importOrder": [
    "^react$",
    "^next/(.*)$",
    "^@/types/(.*)$",
    "^antd(.*)$",
    "^@formily/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^react-icons/(.*)$",
    "^@/assets/(.*)$",
    "^@/components/(.*)$",
    "@/(.*)",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
